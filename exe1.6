import cv2
import numpy as np
cap = cv2.VideoCapture("C:/Users/SAI SUBHASH/Videos/Stranger Things/s0408.mkv")
if not cap.isOpened():
    print("Error opening video file")
while cap.isOpened():
    ret, frame = cap.read()
    if ret:
        cv2.imshow('Frame', frame)
        if cv2.waitKey(250) & 0xFF == ord('q'):
            break
    else:
        break
cap.release()
cv2.destroyAllWindows()
